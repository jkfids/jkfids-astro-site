---
import SocialIcon from "@components/SocialIcon.astro";
import { SITE } from "@consts";

export interface Props {
    icon_size: string
}
  
const { icon_size } = Astro.props;
---
<ul class="not-prose flex flex-wrap gap-2.5">
    <SocialIcon icon_size={icon_size} URL="mailto:john.kam@monash.edu" icon="envelope-fill"/>
    <SocialIcon icon_size={icon_size} URL="https://www.linkedin.com/in/john-kam-b8a486188/" icon="linkedin"/>
    <SocialIcon icon_size={icon_size} URL="https://github.com/jkfids" icon="github"/>
    <SocialIcon icon_size={icon_size} URL="https://scholar.google.com.au/citations?user=CfBWcfUAAAAJ&h" icon="mortarboard-fill"/>
    <SocialIcon icon_size={icon_size} URL={`${SITE.SITEURL}/rss.xml`} icon="rss-fill"/>
    <!-- <SocialIcon icon_size={icon_size} URL="#" icon="twitter-x"/> -->
</ul>
